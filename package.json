{
  "name": "ai-governor-ci",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "ci:validate:rules": "node scripts/ci/validate-rules.js",
    "test": "npm --prefix src/frontend/app run test",
    "test:run": "npm --prefix src/frontend/app run test:run",
    "dev:frontend": "npm --prefix src/frontend/app run dev",
    "build:frontend": "npm --prefix src/frontend/app run build",
    "openapi:generate:core": "npx @openapitools/openapi-generator-cli generate -i openapi/api-v1.0.yaml -g nodejs-express-server -o services/core-stub --additional-properties=useES6=true,serverPort=3000"
  },
  "devDependencies": {
    "ajv-cli": "^0.6.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "swagger-cli": "^4.0.4",
    "@openapitools/openapi-generator-cli": "^2.13.4"
  }
}
